doctype html
html
   head
      title SRR
      meta(charset='utf-8')
      meta(name='viewport' content='width=device-width,initial-scale=1.0')
      link(rel='icon' href='favicon.png')
      link(rel='stylesheet' href='styles.sass')
      base(target='_blank')

   body
      .srr-container
         .srr-panel.vPanel.hidden
            .srr-selector

         .srr-feed.vFeed.hidden
            .srr-header
               .srr-in
                  a.srr-link
                     .srr-title
               .srr-info
                  .srr-website
                  .srr-tag
                  .srr-published
            .srr-content

         .srr-controls
            button.srr-left.vFeed.hidden(disabled) <
            button.srr-right.vFeed.hidden(disabled) >
            button.srr-last.vFeed.hidden >|
            button.srr-menu M

      script(type='text/javascript').
         PACKS_URL = "#{process.env.SRR_CDN_URL}"
         DB = fetch(PACKS_URL + "/db.json")

      script(src='js/app.ts', type="module")